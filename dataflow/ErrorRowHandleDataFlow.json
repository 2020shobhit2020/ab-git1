{
	"name": "ErrorRowHandleDataFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "salesINDdataset",
						"type": "DatasetReference"
					},
					"name": "salesIND"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "ConditionalSplit",
					"description": "conditionalsplit"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> salesIND",
				"salesIND split(isNull(toDate(saledate,'dd-mm-yyyy')),",
				"     disjoint: false) ~> ConditionalSplit@(ErrorRows, CorrectRows)"
			]
		}
	}
}